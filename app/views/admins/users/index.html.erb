<div class="container">
  <div class="col-lg-offset-1 col-lg-10">
    <table class="table table-hover">
      <thead>
        <tr>
          <th></th>
          <th>id</th>
          <th>ユーザ</th>
        </tr>
      </thead>
        <tbody>
          <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.nick_name %></td>
            <td><%= user.user_name %></td>
            <td><%=link_to '詳細' , admins_user_path(user.id) %></td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>